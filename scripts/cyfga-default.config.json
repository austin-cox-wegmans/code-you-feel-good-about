{
  "baseDirectory": "./",
  "coverage": {
    "paths": ["./"],
    "extensions": [".js", ".ts", ".jsx", ".tsx"],
    "exclude": []
  },
  "excludeDirectories": ["node_modules"],
  "profiles": [
    {
      "name": "commerce-web",
      "tests": { "paths": ["/components", "/hooks", "/stores", "/utilities"] },
      "coverage": {
        "paths": ["/components", "/hooks", "/stores", "/utilities"]
      }
    },
    {
      "name": "commerce-app",
      "tests": { "paths": ["/__tests__"] },
      "coverage": { "paths": ["/src"] }
    },
    { "name": "commerce-austin", "overrides": { "tests": { "paths": [] } } }
  ],
  "tests": {
    "paths": ["./"],
    "extensions": [".js", ".ts", ".jsx", ".tsx"],
    "exclude": []
  },
  "_profile": ""
}
